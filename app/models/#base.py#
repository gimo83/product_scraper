import json
from app import db

class 


class Base(json.JSONEncoder):
    def __init__(self):
        self._db = db.table(self._table) 

    def default(self, obj):
        return o.__dict__
    
    def find(self):
        pass

    def save(self):
        #print(json.dumps(self))
        if '_id' in self.__dict__:
            self._db.update(json.dump(self))
        else:
            self._db.insert(json.dumps(self))
